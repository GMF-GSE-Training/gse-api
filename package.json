  "description": "A backend project for web-based certificate management",
  "author": "",
  "private": true,
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/AminudinAbdulloh/backend-projek-sertifikat-berbasis-web.git"
  },
  "bugs": {
    "url": "https://github.com/AminudinAbdulloh/backend-projek-sertifikat-berbasis-web/issues"
