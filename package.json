    "migrate:dev": "NODE_ENV=development ts-node -r dotenv/config migrate.ts dotenv_config_path=.env.development",
    "migrate:stag": "NODE_ENV=staging ts-node -r dotenv/config migrate.ts dotenv_config_path=.env.staging",
