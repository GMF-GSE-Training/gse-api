<div style="padding: 20px;">
    <h1 style="font-size: 24px; color: #333;">Daily File Upload Notification Summary</h1>
    <p style="font-size: 16px; color: #333; line-height: 1.5;">
        Berikut adalah ringkasan notifikasi untuk tanggal <%= date %>:
    </p>

    <% if (hasSensitive) { %>
        <h2 style="font-size: 20px; color: #333; margin-top: 20px;">
            Sensitive File Uploads (<%= sensitiveNotifications.length %>)
        </h2>
        <ul style="font-size: 16px; color: #333; line-height: 1.5; padding-left: 20px;">
            <% sensitiveNotifications.forEach(notification => { %>
                <li><%= notification %></li>
            <% }) %>
        </ul>
    <% } %>

    <% if (hasFailures) { %>
        <h2 style="font-size: 20px; color: #333; margin-top: 20px;">
            Upload Failures (<%= failureNotifications.length %>)
        </h2>
        <ul style="font-size: 16px; color: #333; line-height: 1.5; padding-left: 20px;">
            <% failureNotifications.forEach(notification => { %>
                <li><%= notification %></li>
            <% }) %>
        </ul>
    <% } %>

    <p style="font-size: 16px; color: #333; line-height: 1.5; margin-top: 20px;">
        Terima kasih,
    </p>
    <p style="font-size: 16px; color: #333; line-height: 1.5;">
        <b>GMF Aeroasia</b>
    </p>
</div>